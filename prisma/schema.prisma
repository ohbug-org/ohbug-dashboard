datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Event {
  id           String   @id @default(uuid())
  apiKey       String
  appVersion   String?
  appType      String?
  releaseStage String?
  timestamp    DateTime
  category     String?
  type         String
  sdk          Json
  detail       Json
  device       Json
  user         Json?
  actions      Json?
  metaData     Json?
  intro        String
  createdAt    DateTime @default(now())
}
