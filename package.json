{
  "name": "ohbug-dashboard",
  "packageManager": "pnpm@7.1.0",
  "homepage": "https://github.com/ohbug-org/ohbug-dashboard#readme",
  "bugs": {
    "url": "https://github.com/ohbug-org/ohbug-dashboard/issues"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "chenyueban",
    "email": "jasonchan0527@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ohbug-org/ohbug-dashboard.git"
  },
  "funding": "https://github.com/sponsors/chenyueban",
  "sideEffects": false,
  "scripts": {
    "postinstall": "nr docker && nr setup",
    "preinstall": "npx only-allow pnpm",
    "update:deps": "pnpm update -i -r --latest",
    "prepare": "husky install",
    "build": "pnpm -r run build",
    "docker": "docker-compose up -d",
    "dev": "nr docker & pnpm --parallel -r run dev",
    "dev:web": "nr docker & pnpm --parallel --filter web --filter web run dev",
    "dev:server": "nr docker & pnpm --parallel --filter server --filter config run dev",
    "setup": "prisma migrate dev && prisma db seed",
    "studio": "prisma studio",
    "lint": "eslint . --fix",
    "prepublishOnly": "nr build",
    "test": "pnpm -r run test"
  },
  "dependencies": {
    "@ohbug/browser": "^2.0.3",
    "@ohbug/core": "^2.0.3",
    "@ohbug/react": "^2.0.3",
    "@ohbug/types": "^2.0.3",
    "@ohbug/vue": "^2.0.3",
    "@prisma/client": "^4.0.0",
    "dayjs": "^1.11.3",
    "dotenv": "^16.0.1",
    "ramda": "^0.28.0",
    "source-map-trace": "^0.2.6"
  },
  "devDependencies": {
    "@antfu/ni": "^0.16.3",
    "@babel/types": "^7.18.8",
    "@changesets/cli": "^2.23.1",
    "@chenyueban/eslint-config": "^1.0.12",
    "@chenyueban/tsconfig": "^2.1.0",
    "@commitlint/cli": "^17.0.3",
    "@types/node": "^18.0.3",
    "@types/ramda": "^0.28.15",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.19.0",
    "esno": "^0.16.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prisma": "^4.0.0",
    "rimraf": "^3.0.2",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4"
  },
  "prisma": {
    "seed": "esno prisma/seed.ts"
  }
}
